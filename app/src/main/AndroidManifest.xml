<?xml version="1.0" encoding="utf-8"?><!--<a href="https://www.vecteezy.com/free-vector/notebook-icon">Notebook Icon Vectors by Vecteezy</a>-->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yash.simplenotes">

    <application
        android:name=".NotesApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_simplenotes"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_simplenotes_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimpleNotes">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>